package com.acme.model;

import java.util.ArrayList;
import java.util.HashMap;

public class Employee {
	private String name;
	private HashMap<String, ArrayList<String>> work;

	public Employee() {
	}

	public Employee(String name, HashMap<String, ArrayList<String>> work) {
		super();
		this.name = name;
		this.work = work;
	}

	public HashMap<String, ArrayList<String>> getWork() {
		return work;
	}

	public void setWork(HashMap<String, ArrayList<String>> work) {
		this.work = work;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
